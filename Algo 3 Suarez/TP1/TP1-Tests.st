TestCase subclass: #AlgoPrediccionCriptoTest	instanceVariableNames: 'precision'	classVariableNames: ''	package: 'TP1-Tests'!!AlgoPrediccionCriptoTest methodsFor: 'running' stamp: 'mateo 3/12/2025 10:59'!setUp	super setUp.	precision := 1e-3.	"Put here a common initialization logic for tests"! !!AlgoPrediccionCriptoTest methodsFor: '*TP1-Casos-de-Uso' stamp: 'DanaKolln 4/11/2025 23:43'!test09AlgoPrediccionCriptoLanzaErrorConColorDeCabelloInvalido		|  algoPrediccionCripto |		"Arrange"		algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		"Act y Assert"	self should:[		algoPrediccionCripto registrarTokenConNombre: '$MASIVO' ConValorActual: 10 ConRecomendador: 'Matias' yEdad: 21 yPatrimonio: 50000 deNacionalidad: 'Argentina' yColorDePelo: 'Negro'.]	 raise: ColorInvalidoError.		! !!AlgoPrediccionCriptoTest methodsFor: '*TP1-Casos-de-Uso' stamp: 'DanaKolln 4/16/2025 16:42'!test10AlgoPrediccionCriptoLanzaErrorConRegistroVacio	| algoPrediccionCripto  |		"Arrange"		algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.			"Act/Assert"		self should:[algoPrediccionCripto obtenerDivulgadorSegunCriterio]	 raise: RegistroVacioError.		 		! !!AlgoPrediccionCriptoTest methodsFor: '*TP1-Casos-de-Uso' stamp: 'DanaKolln 4/12/2025 00:51'!test08AlgoPrediccionCriptoLanzaErrorConCriterioInvalido	self should:[AlgoPrediccionCripto  conCriterio: 'Alto'] raise: CriterioInvalidoError.! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 23:05'!test03UnRecomendadorMorochoConCiertaEdadyPatrimonioPuedePredecirUnAumentoDeCiertaCriptoDeCiertoValor	| prediccionEsperada prediccionObtenida algoPrediccionCripto  |		"Arrange"		prediccionEsperada := 2865.6.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		"El recomendador es de un pais extranjero por lo que tiene gran veracidad. Su valor equivale al 110%."	"Debido a que es morocho, la cripto puede aumentar su edad multiplicado por su patrimonio dividido 10000000"	algoPrediccionCripto registrarTokenConNombre: '$X AE A-XII' ConValorActual: 1000 ConRecomendador: 'Elon' yEdad: 53 yPatrimonio: 320000000 deNacionalidad: 'Sudafrica' yColorDePelo: 'Morocho'.		"Act"		prediccionObtenida := algoPrediccionCripto prediccionDe: '$X AE A-XII' conRecomendador: 'Elon'.		"Assert"	self assert: (prediccionObtenida compararCon: prediccionEsperada precision: precision )! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 23:05'!test01UnRecomendadorArgentinoConCiertoColorDePeloPatrimonioYEdadPuedePredecirUnAumentoDeCiertaCriptoDeCiertoValor		| prediccionEsperada prediccionObtenida algoPrediccionCripto  |		"Arrange"		prediccionEsperada := 3807.25.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		"El recomendador es de Argentina, por lo que pierde veracidad. Su valor equivale al 75%." 	"Debido a que es colorado, la cripto puede aumentar el patrimonio dividido 10 mas el triple de su edad"	algoPrediccionCripto registrarTokenConNombre: '$MASIVO' ConValorActual: 10 ConRecomendador: 'Matias' yEdad: 21 yPatrimonio: 50000 deNacionalidad: 'Argentina' yColorDePelo: 'Colorado'.		"Act"		prediccionObtenida := algoPrediccionCripto prediccionDe: '$MASIVO' conRecomendador: 'Matias'.			"Assert"	self assert: (prediccionObtenida compararCon: prediccionEsperada precision: precision )! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 23:05'!test07UnRecomendadorConCiertoColorDePeloPatrimonioYEdadPuedePredecirUnAumentoDeCiertaCriptoDeCiertoValorEnPesos	| prediccionEsperadaEnPesos algoPrediccionCripto prediccionObtenida |		"Arrange"		prediccionEsperadaEnPesos := 4568700.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		algoPrediccionCripto registrarTokenConNombre: '$MASIVO' ConValorActual: 10 ConRecomendador: 'Matias' yEdad: 21 yPatrimonio: 50000 deNacionalidad: 'Argentina' yColorDePelo: 'Colorado'.		"Act"		prediccionObtenida := algoPrediccionCripto prediccionDe: '$MASIVO' conRecomendador: 'Matias'.			"Assert"	"Prediccion en pesos = Prediccion en dolares Ã— 1200"	self assert: (prediccionObtenida compararPrediccionEnPesosCon: prediccionEsperadaEnPesos  precision: precision )! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 22:52'!test05EntreVariosRecomendadoresDetectoAlQuePrediceUnMenorValor	| divulgadorMenorGananciaEsperado divulgadorMenorGananciaObtenido algoPrediccionCripto  |		"Arrange"		divulgadorMenorGananciaEsperado := 'Javier'.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Minimo'.		algoPrediccionCripto registrarTokenConNombre: '$MASIVO' ConValorActual: 10 ConRecomendador: 'Matias' yEdad: 21 yPatrimonio: 50000 deNacionalidad: 'Argentina' yColorDePelo: 'Colorado'.		algoPrediccionCripto registrarTokenConNombre: '$BITCOIN' ConValorActual: 1000 ConRecomendador: 'Elon' yEdad: 53 yPatrimonio: 320000000 deNacionalidad: 'Sudafrica' yColorDePelo: 'Morocho'.		algoPrediccionCripto registrarTokenConNombre: '$BITCOIN' ConValorActual: 1000 ConRecomendador: 'Donald' yEdad: 78 yPatrimonio: 300000000 deNacionalidad: 'Estados Unidos' yColorDePelo: 'Rubio'.		algoPrediccionCripto registrarTokenConNombre: '$LIBRA' ConValorActual: 1 ConRecomendador: 'Javier' yEdad: 54 yPatrimonio: 10000000 deNacionalidad: 'Argentina' yColorDePelo: 'Morocho'.	"Act"		divulgadorMenorGananciaObtenido := algoPrediccionCripto obtenerDivulgadorSegunCriterio.		"Assert"	self assert: divulgadorMenorGananciaEsperado equals: divulgadorMenorGananciaObtenido! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 22:51'!test04EntreUnRecomendadorYOtroDetectaAlQuePrediceUnMayorValor	| divulgadorMayorGananciaEsperado divulgadorMayorGananciaObtenido algoPrediccionCripto  |		"Arrange"		divulgadorMayorGananciaEsperado := 'Matias'.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		algoPrediccionCripto registrarTokenConNombre: '$PLEBEYO' ConValorActual: 10 ConRecomendador: 'Matias' yEdad: 21 yPatrimonio: 50000 deNacionalidad: 'Argentina' yColorDePelo: 'Colorado'.		algoPrediccionCripto registrarTokenConNombre: '$TESLA' ConValorActual: 1000 ConRecomendador: 'Elon' yEdad: 53 yPatrimonio: 320000000 deNacionalidad: 'Sudafrica' yColorDePelo: 'Morocho'.		"Act"		divulgadorMayorGananciaObtenido := algoPrediccionCripto obtenerDivulgadorSegunCriterio.		"Assert"	self assert: divulgadorMayorGananciaEsperado equals: divulgadorMayorGananciaObtenido! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 23:05'!test02UnRecomendadorExtranjeroConCiertoColorDePeloPatrimonioYEdadPuedePredecirUnAumentoDeCiertaCriptoDeCiertoValor			| prediccionEsperada prediccionObtenida algoPrediccionCripto  |		"Arrange"		prediccionEsperada := 314986000.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		"El recomendador es de un pais extranjero por lo que tiene gran veracidad. Su valor equivale al 110%."	"Debido a que es rubio, la cripto puede aumentar su patrimonio menos el producto entre su edad y 175000."	algoPrediccionCripto registrarTokenConNombre: '$MAGA' ConValorActual: 1000 ConRecomendador: 'Donald' yEdad: 78 yPatrimonio: 300000000 deNacionalidad: 'Estados Unidos' yColorDePelo: 'Rubio'.		"Act"		prediccionObtenida := algoPrediccionCripto prediccionDe: '$MAGA' conRecomendador: 'Donald'.		"Assert"	self assert: (prediccionObtenida compararCon: prediccionEsperada precision: precision )! !!AlgoPrediccionCriptoTest methodsFor: 'tests' stamp: 'Joaquin 4/1/2025 22:54'!test06EntreVariosRecomendadoresDetectaAlQuePrediceUnMayorValor	| divulgadorMayorGananciaEsperado divulgadorMayorGananciaObtenido algoPrediccionCripto  |		"Arrange"		divulgadorMayorGananciaEsperado := 'Donald'.	algoPrediccionCripto  := AlgoPrediccionCripto conCriterio: 'Maximo'.		algoPrediccionCripto registrarTokenConNombre: '$MASIVO' ConValorActual: 10 ConRecomendador: 'Matias' y