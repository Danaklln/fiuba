Object subclass: #AlgoPrediccionCripto	instanceVariableNames: 'registroCriptos criterio creadorDePrediccion'	classVariableNames: ''	package: 'TP1'!!AlgoPrediccionCripto methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/10/2025 19:30'!registrarTokenConNombre: unNombre ConValorActual: unNumero ConRecomendador: unRecomendador yEdad: unaEdad yPatrimonio: unPatrimonio deNacionalidad: unaNacionalidad yColorDePelo: unColor 	|prediccion|		prediccion := creadorDePrediccion crearPrediccionConValor: unNumero recomendador: unRecomendador edad:unaEdad patrimonio:unPatrimonio nacionalidad:unaNacionalidad yColorDePelo: unColor.	registroCriptos registrarPrediccion: prediccion deToken:unNombre.	! !!AlgoPrediccionCripto methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/11/2025 22:35'!inicializarCriterio:unaCategoria	criterio := Criterio deCategoria: unaCategoria.	! !!AlgoPrediccionCripto methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/12/2025 00:45'!initialize 	registroCriptos := Registro new.	creadorDePrediccion := CreadorPrediccion new.! !!AlgoPrediccionCripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/17/2025 18:59'!prediccionDe: unToken conRecomendador: unNombre 	^ registroCriptos prediccionDe: unToken conRecomendador:unNombre.! !!AlgoPrediccionCripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 22:55'!obtenerDivulgadorSegunCriterio	^criterio obtenerPrediccion: registroCriptos .	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AlgoPrediccionCripto class	instanceVariableNames: ''!!AlgoPrediccionCripto class methodsFor: 'not defined category' stamp: 'DanaKolln 4/8/2025 22:28'!conCriterio: unCriterio	|algoPrediccion|	algoPrediccion := self new.	algoPrediccion inicializarCriterio:unCriterio.	^algoPrediccion.! !Object subclass: #Cabello	instanceVariableNames: 'color'	classVariableNames: ''	package: 'TP1'!!Cabello methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/11/2025 23:17'!inicializarColor: unColor	color := unColor.! !!Cabello methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/10/2025 22:11'!modificar: unaPrediccion conEdad:unaEdad	color modificar: unaPrediccion  conEdad:unaEdad.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Cabello class	instanceVariableNames: ''!!Cabello class methodsFor: 'accessing' stamp: 'DanaKolln 4/11/2025 23:19'!definirColor:unColor	(unColor asLowercase = 'colorado')ifTrue: [  ^(Colorado new) ].	(unColor asLowercase = 'rubio')ifTrue: [ ^ (Rubio new) ].	(unColor  asLowercase = 'morocho')ifTrue:[ ^(Morocho new)].	ColorInvalidoError signal! !!Cabello class methodsFor: 'accessing' stamp: 'DanaKolln 4/11/2025 23:08'!color: unColor	|cabello|	cabello := self new.	cabello inicializarColor: (self definirColor:unColor).	^cabello.! !Error subclass: #ColorInvalidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Colorado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Colorado methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:18'!modificar: unaPrediccion conEdad:unaEdad	unaPrediccion multiplicate: (1/10).	unaPrediccion sumate: 3*unaEdad.	! !Object subclass: #CreadorPrediccion	instanceVariableNames: 'cabello nacionalidad'	classVariableNames: ''	package: 'TP1'!!CreadorPrediccion methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/10/2025 20:14'!definirColor: unColor	cabello := Cabello color: unColor.! !!CreadorPrediccion methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/17/2025 16:49'!definirPais: unaNacionalidad	nacionalidad := Nacionalidad  delPais: unaNacionalidad.! !!CreadorPrediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/17/2025 16:49'!crearPrediccionConValor: unNumero recomendador: unRecomendador edad:unaEdad patrimonio:unPatrimonio nacionalidad:unaNacionalidad yColorDePelo: unColor	| prediccion|	prediccion:= Prediccion conPredictor: unRecomendador  valor:unNumero  yBonus: unPatrimonio.		self definirColor: unColor.	self definirPais: unaNacionalidad .		cabello modificar: prediccion conEdad:unaEdad.	nacionalidad modificar:prediccion.		^prediccion.! !Object subclass: #Cripto	instanceVariableNames: 'predicciones nombre'	classVariableNames: ''	package: 'TP1'!!Cripto methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/15/2025 09:05'!registrarPrediccion: unaPrediccion	self verificarNombreNoRepetido:unaPrediccion.	predicciones add: unaPrediccion.! !!Cripto methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/7/2025 16:49'!initialize 	predicciones := OrderedCollection new.		! !!Cripto methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/8/2025 21:25'!inicializarConNombre: unNombre	nombre := unNombre.! !!Cripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/8/2025 22:25'!tuNombreEs: unNombre	^(nombre = unNombre)! !!Cripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/16/2025 16:33'!prediccionDe: unNombre	|prediccionBuscada |	prediccionBuscada := predicciones detect:[:prediccion | prediccion tuPredictorEs: unNombre]	ifNone:[PrediccionNoEncontradaError signal].	^prediccionBuscada.! !!Cripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/8/2025 22:46'!obtenerMinimaPrediccion	| minimaPrediccion|	minimaPrediccion := predicciones  detectMin:[:minimo | minimo obtenerValor].	^minimaPrediccion.! !!Cripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/8/2025 22:44'!obtenerMaximaPrediccion	| maximaPrediccion|	maximaPrediccion := predicciones  detectMax:[:maximo | maximo obtenerValor].	^maximaPrediccion.! !!Cripto methodsFor: 'not defined category' stamp: 'DanaKolln 4/15/2025 09:14'!verificarNombreNoRepetido:unaPrediccion	(predicciones anySatisfy: [:prediccion | prediccion compararPredictor:unaPrediccion])	ifTrue:[PredictorRepetidoError signal]	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Cripto class	instanceVariableNames: ''!!Cripto class methodsFor: 'not defined category' stamp: 'DanaKolln 4/8/2025 21:24'!conNombre: unNombre	|cripto|	cripto := self new.	cripto inicializarConNombre: unNombre.	^cripto.! !Error subclass: #CriptoNoEncontradaError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Criterio	instanceVariableNames: 'categoria'	classVariableNames: ''	package: 'TP1'!!Criterio methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/11/2025 22:39'!inicializarCategoria:unaCategoria	categoria := unaCategoria.! !!Criterio methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 22:44'!obtenerPrediccion:unRegistro	^categoria obtenerPrediccion: unRegistro.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Criterio class	instanceVariableNames: ''!!Criterio class methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 22:47'!definirCategoria:unaCategoria	(unaCategoria asLowercase  = 'maximo') 	ifTrue:[^(Maximo new)].	(unaCategoria asLowercase = 'minimo')	ifTrue:[^(Minimo new)].	CriterioInvalidoError signal! !!Criterio class methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 22:38'!deCategoria:unCriterio	|criterio|	criterio := self new.	criterio inicializarCategoria:(self definirCategoria:unCriterio).	^criterio! !Error subclass: #CriterioInvalidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Maximo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Maximo methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 22:51'!obtenerPrediccion:unRegistro	^unRegistro obtenerMaximaPrediccion ! !Object subclass: #Minimo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Minimo methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 22:51'!obtenerPrediccion:unRegistro	^unRegistro obtenerMinimaPrediccion ! !Object subclass: #Morocho	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Morocho methodsFor: 'not defined category' stamp: 'DanaKolln 4/17/2025 17:32'!modificar: unaPrediccion conEdad:unaEdad	unaPrediccion multiplicate: ((1/10000000) * unaEdad).	! !Object subclass: #Nacionalidad	instanceVariableNames: 'pais'	classVariableNames: ''	package: 'TP1'!!Nacionalidad methodsFor: 'accessing' stamp: 'DanaKolln 4/11/2025 22:42'!inicializarPais: unPais	pais:=unPais! !!Nacionalidad methodsFor: 'accessing' stamp: 'DanaKolln 4/10/2025 19:11'!modificar:unaPrediccion	pais modificarSegunVeracidad:unaPrediccion.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Nacionalidad class	instanceVariableNames: ''!!Nacionalidad class methodsFor: 'not defined category' stamp: 'DanaKolln 4/17/2025 16:49'!delPais: unPais	|nacionalidad|	nacionalidad := self new.	nacionalidad inicializarPais: (self definirPais:(unPais asLowercase)).	^nacionalidad.! !!Nacionalidad class methodsFor: 'not defined category' stamp: 'DanaKolln 4/17/2025 16:49'!definirPais:unPais	( unPais = 'argentina')ifTrue:[^(Argentina new)]	ifFalse:[^(Extranjero new)].	! !Object subclass: #Pais	instanceVariableNames: 'modificador'	classVariableNames: ''	package: 'TP1'!!Pais methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/17/2025 22:56'!modificarSegunVeracidad: unaPrediccion	unaPrediccion multiplicate: modificador! !!Pais methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/17/2025 23:03'!initialize 	modificador := self modificador.! !!Pais methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/17/2025 23:02'!modificador	self subclassResponsibility! !Pais subclass: #Argentina	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Argentina methodsFor: 'not defined category' stamp: 'DanaKolln 4/17/2025 23:03'!modificador 	^0.75! !Pais subclass: #Extranjero	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Extranjero methodsFor: 'initialization' stamp: 'DanaKolln 4/17/2025 23:04'!modificador 	^1.10! !Object subclass: #Prediccion	instanceVariableNames: 'predictor valor bonus'	classVariableNames: ''	package: 'TP1'!!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:20'!compararCon: unNumero precision: unaPrecision	^((valor+bonus) closeTo:unNumero precision: unaPrecision).	! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:15'!multiplicate: unNumero	bonus:= bonus * unNumero.! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/15/2025 09:16'!compararPredictor:unaPrediccion	^(unaPrediccion tuPredictorEs: predictor )! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:19'!obtenerValor	^(valor + bonus)! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:15'!sumate: unNumero	bonus:= bonus + unNumero.! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:05'!inicializarConPredictor: unNombre valor:unNumero yBonus:unBonus	predictor:= unNombre.	valor := unNumero.	bonus := unBonus! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:20'!compararPrediccionEnPesosCon: unNumero precision:unaPrecision	^(((valor + bonus) * 1200) closeTo:unNumero precision: unaPrecision).! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/8/2025 22:42'!obtenerNombre	^predictor ! !!Prediccion methodsFor: 'not defined category' stamp: 'DanaKolln 4/7/2025 16:56'!tuPredictorEs: unNombre	^(predictor asLowercase = unNombre asLowercase )! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Prediccion class	instanceVariableNames: ''!!Prediccion class methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:05'!conPredictor: unNombre valor:unNumero yBonus: unBonus	|prediccion|	prediccion := self new.	prediccion inicializarConPredictor: unNombre valor:unNumero yBonus:unBonus.	^prediccion.! !Error subclass: #PrediccionNoEncontradaError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #PredictorRepetidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Registro	instanceVariableNames: 'criptos'	classVariableNames: ''	package: 'TP1'!!Registro methodsFor: 'as yet unclassified' stamp: 'DanaKolln 4/8/2025 22:24'!initialize 	criptos := OrderedCollection new.! !!Registro methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 23:28'!registrarPrediccion: prediccion deToken:unNombre	|criptoBuscada |		criptoBuscada := criptos detect:[:cripto | cripto tuNombreEs: unNombre]		ifNone:[criptoBuscada:= (self registrarCripto: unNombre)].		criptoBuscada registrarPrediccion: prediccion.	! !!Registro methodsFor: 'not defined category' stamp: 'DanaKolln 4/12/2025 01:06'!obtenerMinimaPrediccion	|minimos minimaPrediccion|	self verificarRegistroNoVacio.	minimos := criptos collect:[:cripto | cripto obtenerMinimaPrediccion].	minimaPrediccion := minimos detectMin:[:minimo | minimo obtenerValor].	^minimaPrediccion obtenerNombre.	! !!Registro methodsFor: 'not defined category' stamp: 'DanaKolln 4/12/2025 01:05'!obtenerMaximaPrediccion	|maximos maximaPrediccion|	self verificarRegistroNoVacio.	maximos := criptos collect:[:cripto | cripto obtenerMaximaPrediccion].	maximaPrediccion := maximos detectMax:[:maximo | maximo obtenerValor].	^maximaPrediccion obtenerNombre.	! !!Registro methodsFor: 'not defined category' stamp: 'DanaKolln 4/16/2025 16:40'!prediccionDe: unaCripto conRecomendador: unNombre	|criptoBuscada |	criptoBuscada := criptos detect:[:cripto | cripto tuNombreEs: unaCripto]	ifNone:[CriptoNoEncontradaError signal].	^(criptoBuscada prediccionDe:unNombre).! !!Registro methodsFor: 'not defined category' stamp: 'DanaKolln 4/11/2025 23:28'!registrarCripto: unNombre	|cripto|	cripto := Cripto conNombre:unNombre.	criptos add: cripto.	^cripto	! !!Registro methodsFor: 'not defined category' stamp: 'DanaKolln 4/16/2025 16:42'!verificarRegistroNoVacio	(criptos isEmpty) ifTrue:  [RegistroVacioError signal]! !Error subclass: #RegistroVacioError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Rubio	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Rubio methodsFor: 'not defined category' stamp: 'DanaKolln 4/10/2025 22:18'!modificar: unaPrediccion conEdad:unaEdad	unaPrediccion sumate: (-1*(unaEdad * 175000)).	! !